uwsgi --socket=127.0.0.1:{{ host_port }} --chdir=/home/{{ account }}/{{ project_dir }}/{{ setting_dir }} --pythonpath=/home/{{ account }}/{{ project_dir }} --virtualenv=/home/{{ account }}/env --env DJANGO_SETTINGS_MODULE=settings --module="django.core.handlers.wsgi:WSGIHandler()" --harakiri=120 --processes=1 --async 100 --ugreen --daemonize=/home/{{ account }}/logs/uwsgi.log --touch-reload=/home/{{ account }}/uwsgi_reload